<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App3.HomePage">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="tstSQL"    Style="{StaticResource buttonStyle}" Text="SQLite" Clicked="tstSQL_Clicked"/>
            <Button x:Name="tstMap"    Style="{StaticResource buttonStyle}" Text="Map" Clicked="tstMap_Clicked"/>
            <Button x:Name="btnCamera" Style="{StaticResource buttonStyle}" Text="Camera" Clicked="btnCamera_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>